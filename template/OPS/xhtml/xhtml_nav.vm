<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="ja" lang="ja">
<head>
<title>${bookInfo.Title}</title>
<style type="text/css">
.center {text-align:center;}
h1 {font-size:1.5em;}
ol {text-decoration:underline;line-height:1.5em;}
</style>
</head>
<body>
	<nav xmlns:epub="http://www.idpf.org/2007/ops" epub:type="toc" id="toc">
		<div class="center"><h1>${toc_name}</h1></div>
		<ul>
#if (${bookInfo.InsertCoverPage})
			<li class="chapter" id="toccover"><a href="cover.xhtml">${cover_name}</a></li>
#end
#foreach(${chapter} in ${chapters})
#if ($chapter.ChapterName)
			<li class="chapter" id="toc${chapter.SectionId}"><a href="${chapter.SectionId}.xhtml">${chapter.ChapterName}</a></li>
#end
#end
		</ul>
	</nav>
</body>
</html>
